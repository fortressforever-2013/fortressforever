"LightmappedGeneric"
{
	"%keywords" "FF"













	"$abovewater" "1"
	"%compilewater" "1"

	"$basetexture" "dev/water"


	"$bottommaterial" "ff_cz2/ff_water_canals_dx70_beneath"


	"$surfaceprop" "water"

	"$color" "[.085 .080 .070]"
	"$scale" "[1 1]"
	"$translucent" "1"
	//"$additive" "1"
	"$nocull" "1"


	"$texoffset" "[0 0]"
	"$texscale" "0.5"

	"$fogenable" "1"
	"$fogcolor" "[.085 .080 .070]"
	"$fogstart" "0"
	"$fogend" "1024.00"



	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" "20.00"
		}

		"TextureScroll"
		{
			"texturescrollvar" "$texoffset"
			"texturescrollrate" "0.025"
			"texturescrollangle" "45.00"
		}

		"TextureTransform"
		{
			"translateVar" "$texoffset"
			"scaleVar" "$texscale"
			"resultVar" "$basetexturetransform"
		}
	}
}
